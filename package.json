{
  "name": "angular-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon ./dist/server/bin/www.js",
    "dev": "concurrently --kill-others \"npm run server:run\" \"ng serve --port 4201 --aot --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test --watch false",
    "e2e:client": "ng e2e",
    "build": "rimraf dist && ng build --prod --aot --output-path=dist/client && npm run server:build",
    "server:run": "nodemon",
    "// server:build": "tsc -p ./server",
    "server:build": "webpack",
    "postinstall": "npm run build",
    "temp": "ng build --output-path=dist/client"
  },
  "author": "Raphi Stein",
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "repository": {
    "url": ""
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^9.1.9",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/uuid": "^8.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "buttercms": "^1.2.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.5",
